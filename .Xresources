Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter

XTerm*renderFont: true
XTerm*faceName: Source Code Pro
XTerm*faceSize: 12
XTerm*utf8: 2
XTerm*locale: true
XTerm*BorderWidth: 0
! XTerm*BorderColor: #898989
XTerm*foreground: #ffffff
XTerm*background: #3f3f3f
XTerm*cursorColor: #ffffff
XTerm*cursorBlink: true
! Every shell is a login shell by default (for inclusion of all necessary environment variables)
XTerm*loginshell: true

URxvt*imLocale: en_US.UTF-8
URxvt*termName: rxvt-unicode-256color
Urxvt*buffered: false
URxvt.intensityStyles: false
URxvt.font: xft:Source Code Pro:size=12
URxvt.saveLines: 2000
URxvt.scrollBar: false
URxvt.cursorColor: #fce205
URxvt.cursorBlink: 1

! special
*.foreground:   #ffffff
*.background:   #3f3f3f
*.cursorColor:  #ffffff

! black
*.color0:       #101010
*.color8:       #434758

! red
*.color1:       #f07178
*.color9:       #ff8b92

! green
*.color2:       #c3e88d
*.color10:      #ddffa7

! yellow
*.color3:       #ffcb6b
*.color11:      #ffe585

! blue
*.color4:       #82aaff
*.color12:      #9cc4ff

! magenta
*.color5:       #c792ea
*.color13:      #e1acff

! cyan
*.color6:       #89ddff
*.color14:      #a3f7ff

! white
*.color7:       #d0d0d0
*.color15:      #ffffff
